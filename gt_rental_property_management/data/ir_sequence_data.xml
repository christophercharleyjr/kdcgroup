<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!--Sequences for Tenancy-->
        <record id="seq_tenancy" model="ir.sequence">
            <field name="name">Tenancy</field>
            <field name="code">tenancy</field>
            <field name="prefix">TE/%(year)s-%(month)s-%(day)s/</field>
            <field name="padding">5</field>
        </record>

        <!--Sequences for Issue-->
        <record id="seq_issue" model="ir.sequence">
            <field name="name">Issue</field>
            <field name="code">issue</field>
            <field name="prefix">IS/%(year)s-%(month)s-%(day)s/</field>
            <field name="padding">5</field>
        </record>

        <!--Sequences for Issue Payment-->
        <record id="seq_issue_payment" model="ir.sequence">
            <field name="name">Issue Payment</field>
            <field name="code">issue_pay</field>
            <field name="prefix">IP</field>
            <field name="padding">5</field>
        </record>

        <!--Sequences for Landlord Payment-->
        <record id="seq_landlord_payment" model="ir.sequence">
            <field name="name">Landlord Payment</field>
            <field name="code">landlord_pay</field>
            <field name="prefix">LP</field>
            <field name="padding">5</field>
        </record>
    </data>
</odoo>
